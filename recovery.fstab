# mount point  fstype device                             device2              flags
/boot          emmc   /dev/block/by-name/boot
/cache         ext4   /dev/block/mmcblk0p25
/recovery      emmc   /dev/block/by-name/recovery                     flags=backup=1
/system        ext4   /dev/block/by-name/system
/vendor        ext4   /dev/block/dm-0                       flags=backup=1;display="Vendor"


/data          ext4   /dev/block/dm-1                    


